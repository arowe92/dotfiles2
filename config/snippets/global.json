{
    "BlockComment": {
        "prefix": ["block"],
        "body": [
            "/**",
            " * ${1:Text}",
            " */"
        ],
        "description": "C++ Style Block Comment"
    },
    "ForOf": {
        "prefix": ["forof"],
        "body": [
            "for (const ${1:key} in ${2:data}) {",
            "    const ${3:value} = ${2:data}[${1:key}];",
            "}"
        ],
        "description": "Practical For Of Loop"
    },
    "Test": {
        "prefix": ["test"],
        "body": [
            "// @INLINE_TEST",
            "import { expect } from 'chai';",

            "describe('${1:test_name}', () => {",
            "it('${2:case_name}', () => {",
            "",
            "});",
            "});"
        ],
        "description": "inline test"
    },
    "ignore": {
        "prefix": ["ignore"],
        "body": [
            "// @ts-ignore"
        ],
        "description": "TS ignore"
    },
    "shebang": {
        "prefix": ["shebang"],
        "body": [
            "#!/bin/sh"
        ],
        "description": "Shebang Sh"
    },
    "separator": {
        "prefix": ["separator"],
        "body": [
            "//*********************************************",
            "// ${1:title}",
            "//*********************************************"
        ],
        "description": "Seperator"
    }
}


