# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
  name = Alexander Rowe
  email = alexrowe707@gmail.com

[alias]
  fl = log --oneline --decor --graph
  ci = commit
  l = log
  d = diff
  st = stash
  ff = git pull origin -r 
  co = checkout
  b = browse
  br = branch
  pr = pull-request
  f = fetch
  con = rebase --continue
  ignore = update-index --assume-unchanged
  unignore = update-index --no-assume-unchanged
  ignored = !git ls-files -v | grep "^[[:lower:]]"
  
[push]
	default = simple
